fetch('http://localhost:9000/users/create', {
  method: 'POST',
  body: JSON.stringify({
    user: {
        name: 'Test User',
        email: 'abc@gmail.com',
        password: '123456FTC'
    }
  }),
  headers: {
    'Content-type': 'application/json; charset=UTF-8'
  }
})
.then(res => res.json())
.then(console.log)

fetch('http://localhost:9000/users/update/5d335dc97f5c57091f55e047', {
  method: 'PUT',
  body: JSON.stringify({
    user: {
        name: 'Test Update',
        email: 'cde@gmail.com'
    }
  }),
  headers: {
    'Content-type': 'application/json; charset=UTF-8'
  }
})
.then(res => res.json())
.then(console.log)

fetch('http://localhost:9000/users/delete/5d335dc97f5c57091f55e047', {
  method: 'DELETE'
})
.then(res => res.json())
.then(console.log)

fetch('http://localhost:9000/users/get')
  .then(res => res.json())
  .then(console.log)

fetch('http://localhost:9000/users/get/5d335dc97f5c57091f55e047')
  .then(res => res.json())
  .then(console.log)
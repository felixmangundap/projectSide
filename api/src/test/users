fetch('http://localhost:9000/users/create', {
  method: 'POST',
  body: JSON.stringify({
    user: {
        name: 'Test Update',
        email: 'ABC@gmail.com',
        password: '123456FTC'
    }
  }),
  headers: {
    'Content-type': 'application/json; charset=UTF-8'
  }
})
.then(res => res.json())
.then(console.log)

fetch('http://localhost:9000/users/update/5d337951e3dddb0b4c921107', {
  method: 'PUT',
  body: JSON.stringify({
    user: {
        name: 'Test 1',
    }
  }),
  headers: {
    'Content-type': 'application/json; charset=UTF-8'
  }
})
.then(res => res.json())
.then(console.log)

fetch('http://localhost:9000/users/delete/5d337951e3dddb0b4c921107', {
  method: 'DELETE'
})
.then(res => res.json())
.then(console.log)

fetch('http://localhost:9000/users/get')
  .then(res => res.json())
  .then(console.log)

fetch('http://localhost:9000/users/get/5d337951e3dddb0b4c921107')
  .then(res => res.json())
  .then(console.log)